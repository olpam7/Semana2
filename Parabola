using System;
					
public class Program
{
	public static void Main(String[] args)
	{
		int n, i, r;
		string linea;
		i=-5;
		Console.Write("Se generara los datos a partir de ahora, presione cualquier digito");
	linea=Console.ReadLine();
	n=int.Parse(linea);
	while (i<=5) 
		{
			r=i*i+2*i+1;
			Console.Write("x = y^2 + 2y + 1=, siendo y igual a "+i+" "+r);
			linea=Console.ReadLine();
			i=i+1;
		}
	Console.ReadLine();
	}
}
